#+ summary: Get direct back-links for the given nanopublication
#+ method: GET
#+ pagination: 1000
#+ endpoint_in_url: False

prefix npa: <http://purl.org/nanopub/admin/>

select ?linking_np ?pred where {
  graph npa:networkGraph {
    ?linking_np ?pred <http://www.nextprot.org/nanopubs#NX_P42771-1_PTM-0253_140.RAoMW0xMemwKEjCNWLFt8CgRmg_TGjfVSsh15hGfEmcz4> .
  }
}
